{
  "errors": [
    {
      "BLOCK_NOT_FOUND": {
        "code": 24,
        "message": "Block not found"
      }
    },
    {
      "CONTRACT_NOT_FOUND": {
        "code": 20,
        "message": "Contract not found"
      }
    }
  ],
  "name": "starknet_getNonce",
  "params": [
    {
      "description": "The hash of the requested block, or number (height) of the requested block, or a block tag",
      "name": "block_id",
      "required": true,
      "schema": {
        "BLOCK_ID": {
          "description": "Block hash, number or tag",
          "oneOf": [
            {
              "properties": {
                "block_hash": {
                  "BLOCK_HASH": {
                    "FELT": {
                      "description": "A field element. represented by at most 63 hex digits",
                      "pattern": "^0x(0|[a-fA-F1-9]{1}[a-fA-F0-9]{0,62})$",
                      "title": "Field element",
                      "type": "string"
                    },
                    "title": "Block hash"
                  },
                  "title": "Block hash"
                }
              },
              "required": [
                "block_hash"
              ],
              "title": "Block hash",
              "type": "object"
            },
            {
              "properties": {
                "block_number": {
                  "BLOCK_NUMBER": {
                    "description": "The block's number (its height)",
                    "minimum": 0,
                    "title": "Block number",
                    "type": "integer"
                  },
                  "title": "Block number"
                }
              },
              "required": [
                "block_number"
              ],
              "title": "Block number",
              "type": "object"
            },
            {
              "BLOCK_TAG": {
                "description": "A tag specifying a dynamic reference to a block",
                "enum": [
                  "latest",
                  "pending"
                ],
                "title": "Block tag",
                "type": "string"
              },
              "title": "Block tag"
            }
          ],
          "title": "Block id"
        },
        "title": "Block id"
      }
    },
    {
      "description": "The address of the contract whose nonce we're seeking",
      "name": "contract_address",
      "required": true,
      "schema": {
        "ADDRESS": {
          "FELT": {
            "description": "A field element. represented by at most 63 hex digits",
            "pattern": "^0x(0|[a-fA-F1-9]{1}[a-fA-F0-9]{0,62})$",
            "title": "Field element",
            "type": "string"
          },
          "title": "Address"
        },
        "title": "Address"
      }
    }
  ],
  "result": {
    "description": "The contract's nonce at the requested state",
    "name": "result",
    "schema": {
      "FELT": {
        "description": "A field element. represented by at most 63 hex digits",
        "pattern": "^0x(0|[a-fA-F1-9]{1}[a-fA-F0-9]{0,62})$",
        "title": "Field element",
        "type": "string"
      },
      "title": "Field element"
    }
  },
  "summary": "Get the nonce associated with the given address in the given block"
}